package oops;
import java.io.*;
import java.util.*;
public class q4 {
	public static void main(String args[])throws Exception {
		FileWriter fw=new FileWriter("C:\\Users\\HP\\Desktop\\newfile.txt",true);
		fw.write("welcome");
		fw.close();
		FileReader fr=new FileReader("C:\\Users\\HP\\Desktop\\newfile.txt");
		BufferedReader br=new BufferedReader(fr);
		String line;
		while((line=br.readLine())!=null)
				{
			      System.out.println(line);
				}
		fr.close();
	}

}
